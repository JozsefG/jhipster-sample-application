<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity XGroup.
    -->
    <changeSet id="20180731000123-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="x_team_id"
                                 baseTableName="x_group"
                                 constraintName="fk_xgroup_x_team_id"
                                 referencedColumnNames="id"
                                 referencedTableName="x_team"/>

        <addForeignKeyConstraint baseColumnNames="xgroups_id"
                                 baseTableName="xgroup_xuser"
                                 constraintName="fk_xgroup_xuser_xgroups_id"
                                 referencedColumnNames="id"
                                 referencedTableName="x_group"/>
        <addForeignKeyConstraint baseColumnNames="xusers_id"
                                 baseTableName="xgroup_xuser"
                                 constraintName="fk_xgroup_xuser_xusers_id"
                                 referencedColumnNames="id"
                                 referencedTableName="x_user"/>
        
    </changeSet>
</databaseChangeLog>
