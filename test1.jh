// an ignored comment
/** not an ignored comment */
entity Organization {
	name String required
}

entity Team {
	name String required
}

entity Group {
	name String required
}

entity XUser {
	name String required
}

entity Role {
	name String required
}

relationship OneToMany {
	Organization{Team} to Team{Organization}
    Team{Group} to Group{Team}
    Team{User} to XUser{Team}
}

relationship ManyToMany {
	Group{User} to XUser{Group}
    Role{User} to XUser{Role}
}

